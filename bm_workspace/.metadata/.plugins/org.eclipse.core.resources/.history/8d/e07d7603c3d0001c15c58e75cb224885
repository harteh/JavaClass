
public class TestForWhile {

	public static void main(String[] args) {
		
		//0~9
		System.out.println("********* 0-9 ************");
		for (int i = 0; i < 10; i++) {
			System.out.print(i);
		}
		
		System.out.println();
		System.out.println("*********** 0-100까지의 합 ************");
		
		int sum = 0;
		for (int i = 0; i < 101; i++) {
			sum += i;
		}
		System.out.println(sum);

	}

}
